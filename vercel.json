{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/chat",
      "methods": ["OPTIONS"],
      "dest": "/index.js"
    },
    {
      "src": "/chat",
      "methods": ["POST"],
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key"
  }
} 